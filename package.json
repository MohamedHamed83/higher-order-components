  "homepage": "https://github.com/klarna/higher-order-components#readme",
  "devDependencies": {
    "react": "^15.3.2",
    "react-context-props": "^0.1.4",
    "react-dom": "^15.3.2",
    "sagui": "8.0.0-rc.0"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-context-props": "^0.1.4"
  },
  "dependencies": {
    "seed-random": "^2.2.0"
  }
